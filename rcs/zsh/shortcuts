--Commands
dots: cd ~/.dots
y: yay
yu: yay -Syu --devel
home: cd ~
fact: wget -qO- randomfunfacts.com | sed -n "/<strong>/{s;^.*<i>\(.*\)</i>.*$;\1;p}"
p4k: python3 ~/.pyscripts/p4k.py
--FilestoVim
xr: ~/.Xresources
sxrc: ~/.config/sxhkd/sxhkdrc1
bsprc: ~/.config/bspwm/bspwmrc
pbar: ~/.config/polybar/config
rng: ~/.config/ranger/rc.conf
vrc: ~/.vimrc
